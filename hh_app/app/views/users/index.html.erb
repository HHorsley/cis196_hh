<table>
	<tr>
		<td>id</td>
		<td>name</td>
		<td>email</td>
		<td>password</td>
	</tr>
	<% @users.each do |user| %>
	<tr>
		<td><%= user.id %></td>
		<td><%= user.name %></td>
		<td><%= user.email %></td>
		<td><%= user.password %></td>
		<td><%= link_to "show user", user_path(user.id) %></td>
		<td><%= link_to "edit user", edit_user_path(user.id) %></td>
		<td><%= button_to "destroy user", user, :confirm => "Are you sure you want to destroy this user?", method: :delete %></td>
	</tr>
	<% end %>
</table>

